<ol class="post-list">
  <li>
    <article class="post">
      <div class="title">
        <a href="{{post_url}}" target="_blank">{{title}}</a>
        <span>({{post_url}})</span>
      </div>
      <div class="meta">
        Posted by {{user.username}} on {{created_at}}
        
        
      </div>
    </article>
  </li>
</ol>